  "author": "Wahyu Tricahyo",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "@vitejs/plugin-react": "^4.4.1",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.25.0",
    "@eslint/js": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0"
  }
}
