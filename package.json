{
  "name": "client-saas",
  "version": "1.0.3",
  "description": "middleware Qoin SAAS API client for web apps",
  "type": "module",
  "main": "./dist/client-saas.umd.js",
  "module": "./dist/client-saas.es.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxhyxtrx/client-saas.git"
  },
  "bugs": {
    "url": "https://github.com/wxhyxtrx/client-saas/issues"
  },
  "homepage": "https://github.com/wxhyxtrx/client-saas#readme",
  "exports": {
    ".": {
      "import": "./dist/client-saas.es.js",
      "require": "./dist/client-saas.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "saas",
    "client-saas",
    "middleware-saas",
    "request-in",
    "request-out",
    "client"
